{
  "manifest_version": 2,

  "name": "Maild",
  "description": "This extension makes embedding emails beatifull",
  "version": "1.0",
  "background": {
      "scripts": ["jquery-2.1.1.min.js", "backgroundscript.js"]
    }, 
  "page_action": {
    "default_icon": "icon.png"
  },
  "permissions": ["tabs", "http://*/*", "background"],
  "content_scripts": [
    {
      "matches": ["https://mail.google.com/*"],
      "js": ["jquery-2.1.1.min.js", "vex.combined.min.js", "contentscript.js"],
      "css": [    
        "style/vex-theme-os.css",
        "style/vex.css"
        ]
    }
  ],
  "web_accessible_resources": [
    "style/vex-theme-os.css",
    "style/vex.css"
  ]
}


